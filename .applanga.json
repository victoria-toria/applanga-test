{
  "app": {
    "base_language": "en",
    "push": {
      "source": [
        {
          "path": "./package-lock.json",
          "file_format": "",
          "tag": "",
          "language": "en"
        },
        {
          "path": "./package.json",
          "file_format": "",
          "tag": "",
          "language": "en"
        },
        {
          "path": "./server.js",
          "file_format": "ember_i18n_json_module",
          "tag": "app:translations.js",
          "language": "en"
        }
      ]
    },
    "pull": {
      "target": [
        {
          "path": "./<language>.json",
          "file_format": "",
          "tag": "",
          "exclude_languages": [
            "en"
          ]
        },
        {
          "path": "./<language>.json",
          "file_format": "",
          "tag": "",
          "exclude_languages": [
            "en"
          ]
        },
        {
          "path": "./<language>.js",
          "file_format": "ember_i18n_json_module",
          "tag": "app:translations.js",
          "exclude_languages": [
            "en"
          ]
        }
      ]
    }
  }
}
{
  "app": {
    "base_language": "en",
    "push": {
      "source": [
        {
          "path": "./.github/workflows/applanga-pull.yml",
          "file_format": "ruby_on_rails_yaml",
          "tag": "app:language.yaml",
          "language": "en"
        }
      ]
    },
    "pull": {
      "target": [
        {
          "path": "./.github/workflows/<language>.yml",
          "file_format": "ruby_on_rails_yaml",
          "tag": "app:language.yaml",
          "exclude_languages": [
            "en"
          ]
        }
      ]
    }
  }
}